<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/bg_content"
    tools:context="com.zoomlee.zoo.ui.activity.PersonListActivity">

    <com.zoomlee.zoo.ui.view.ComplexDividerListView
        android:id="@+id/personList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:dividerColor="@color/divider_color"
        app:thickWidth="14dp"
        app:thinHeight="1dp"
        app:thickHeight="3dp"
        app:thickPadding="72dp"
        tools:listitem="@layout/categories_doc_list_item"
        android:listSelector="@drawable/selectable_item_background"
        tools:showIn="@layout/fragment_categories" />

</FrameLayout>
